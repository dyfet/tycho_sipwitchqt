PROJECT_NAME = "SipWitchQt"
DOXYFILE_ENCODING = UTF-8
OUTPUT_DIRECTORY = doc
OUTPUT_LANGUAGE = English
CREATE_SUBDIRS = NO
OPTIMIZE_OUTPUT_FOR_C = YES
INPUT = ${CMAKE_CURRENT_SOURCE_DIR} ${CMAKE_CURRENT_BINARY_DIR}/config.hpp
RECURSIVE = YES
FILE_PATTERNS = *.hpp *.md LICENSE
EXTRACT_PRIVATE = NO

SEARCHENGINE = YES
FULL_PATH_NAMES = NO
SHORT_NAMES = YES
TAB_SIZE = 4
ALIASES = "license=\par License:\n"
ALPHABETICAL_INDEX = YES
SORT_GROUP_NAMES = YES
SORT_GROUP_NAMES = YES
HAVE_DOT = YES
SHOW_NAMESPACES = YES

GENERATE_DOCSET = YES
DOCSET_FEEDNAME = "SipWitchQt Documentations"
DOCSET_BUNDLE_ID = "com.tychosoft.sipwitchqt"
DOCSET_PUBLISHER_NAME = "com.tychosoft"

GENERATE_LATEX = YES
GENERATE_HTML = YES
GENERATE_HTMLHELP = YES
GENERATE_QHP = YES
QHP_NAMESPACE = com.tychosoft.sipwitchqt
QHP_VIRTUAL_FOLDER = doc

GENERATE_TREEVIEW = YES
USE_PDFLATEX = YES
PDF_HYPERLINKS = YES
